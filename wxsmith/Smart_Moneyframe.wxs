<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="Smart_MoneyFrame">
		<title>Login Form</title>
		<centered>1</centered>
		<icon stock_id="wxART_ADD_BOOKMARK" />
		<size>368,250</size>
		<minsize>368,230</minsize>
		<maxsize>368,230</maxsize>
		<id_arg>0</id_arg>
		<style>wxDEFAULT_FRAME_STYLE|wxSYSTEM_MENU|wxFRAME_NO_TASKBAR</style>
		<object class="wxPanel" name="ID_PANEL1" variable="Panel1" member="yes">
			<pos>0,56</pos>
			<size>528,264</size>
			<style>wxSIMPLE_BORDER|wxRAISED_BORDER|wxTAB_TRAVERSAL</style>
			<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
				<label>Login Form</label>
				<pos>120,24</pos>
				<fg>wxSYS_COLOUR_3DDKSHADOW</fg>
				<font>
					<size>14</size>
					<style>normal</style>
					<weight>bold</weight>
					<underlined>0</underlined>
					<family>swiss</family>
					<face>Sans</face>
				</font>
			</object>
			<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="yes">
				<label>User id</label>
				<pos>48,64</pos>
				<font>
					<size>14</size>
					<style>normal</style>
					<weight>normal</weight>
					<underlined>0</underlined>
					<family>swiss</family>
					<face>Sans</face>
				</font>
			</object>
			<object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="TextLoginId" member="yes">
				<maxlength>10</maxlength>
				<pos>144,64</pos>
				<size>199,27</size>
				<handler function="OnTextLoginIdText" entry="EVT_TEXT" />
			</object>
			<object class="wxStaticText" name="ID_STATICTEXT3" variable="StaticText3" member="yes">
				<label>Password</label>
				<pos>48,120</pos>
				<font>
					<size>14</size>
					<style>normal</style>
					<weight>normal</weight>
					<underlined>0</underlined>
					<family>swiss</family>
					<face>Sans</face>
				</font>
			</object>
			<object class="wxTextCtrl" name="ID_TEXTCTRL2" variable="TextPassword" member="yes">
				<pos>144,120</pos>
				<size>199,27</size>
				<style>wxTE_PASSWORD</style>
			</object>
			<object class="wxButton" name="ID_BUTTON1" variable="Button1" member="yes">
				<label>Login</label>
				<pos>144,184</pos>
				<handler function="OnButton1Click1" entry="EVT_BUTTON" />
			</object>
			<object class="wxButton" name="ID_BUTTON2" variable="Button2" member="yes">
				<label>New user</label>
				<pos>272,184</pos>
				<handler function="OnButton2Click" entry="EVT_BUTTON" />
			</object>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
